{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "prefer-arrow-callback": [
            0
        ],
        "func-names": [
            0
        ],
        "indent": [
            0
        ],
        "no-console": [
            0
        ]
    }
}